/*메일*/
	options
	 emailhost="111.111.111.11" emailport=25 emailsys=SMTP
	    emailid="aa@aa.aa"  ;
	  /* to=("aa@aa.aa" "aa@aa.aa" ) */
	filename outbox email
	   to=("aa@aa.aa"  ) 
	   type='text/html'
	   subject='메일 ';

	ods html
	   body=outbox(no_top_matter no_bottom_matter) /* Mail it! */
	   rs=none;
	title '메일';
	PROC SQL NUMBER;
	SELECT *
	FROM SASHELP.CLASS
	;

	QUIT;

	ods html close;
